{
  "name": "works-boilerplate",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:check": "next lint",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "analyze": "ANALYZE=true pnpm build",
    "build:mobile": "pnpm build && pnpm lighthouse:mobile",
    "lighthouse:mobile": "lhci autorun --config=.lighthouserc.json",
    "clean": "rm -rf .next out node_modules/.cache",
    "check": "pnpm typecheck && pnpm lint",
    "check:types": "tsc --noEmit --pretty",
    "check:deps": "knip",
    "check:i18n": "i18n-check -l src/locales -s en -u src -f next-intl",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/server/db/seed.ts",
    "db:generate": "prisma generate",
    "db:setup-prisma": "echo 'üìã Run prisma/sql/setup-prisma-user.sql in Supabase SQL Editor'",
    "db:pull": "prisma db pull",
    "db:diff": "prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "mobile:validate": "echo 'üì± Checking mobile compliance...' && pnpm typecheck && pnpm lint",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "validate": "pnpm typecheck && pnpm lint && pnpm format:check",
    "i18n:validate": "node scripts/validate-translations.js",
    "i18n:extract": "echo 'üîç Translation key extraction (future implementation)'",
    "i18n:check": "pnpm i18n:validate"
  },
